int maximumPopulation(int** logs, int logsSize, int* logsColSize) {
    int population[101] = {0};

    int maxpopulation = 0;
    int maxYear = 1950;
    int currentpopulation = 0;

    for (int i = 0; i < logsSize; i++){
        int birth = logs[i][0];
        int death = logs[i][1];
        population[birth - 1950]++;
        population[death - 1950]--;
    }    

    for (int i = 0; i <= 100; i++){
        currentpopulation += population[i];
        int Year = 1950 + i;
        if(currentpopulation > maxpopulation){
            maxpopulation = currentpopulation;
            maxYear = Year;
        }else if (currentpopulation == maxpopulation){
            maxYear = (maxYear < Year) ? maxYear : Year;
        }
    }
    return maxYear;
}
