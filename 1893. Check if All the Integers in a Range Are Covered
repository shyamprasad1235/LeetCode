bool isCovered(int** ranges, int rangesSize, int* rangesColSize, int left, int right) {

    int diff[52] = {0};

    for (int i = 0; i < rangesSize; i++){
        int start = ranges[i][0];
        int end = ranges[i][1];
        diff[start]++;
        diff[end + 1]--;        
    }
    int prefix[52] = {0};
    prefix[0] = diff[0];
    for(int i = 1; i <= right; i++){
        prefix[i] = prefix[i-1] + diff[i];
    }

    for (int i = left; i <= right; i++){
        if (prefix[i] <= 0){
            return false;
        }
    }
    return true;    
}
